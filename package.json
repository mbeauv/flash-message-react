{
  "name": "flash-message-react",
  "version": "0.1.0",
  "description": "React component allowing display of flash messages provided by flash-message-redux.",
  "main": "dist/index.js",
  "repository": {
  "type": "git",
  "url": "https://github.com/mbeauv/flash-message-react"
  },
  "scripts": {
    "prebuild": "rimraf dist && mkdir dist",
